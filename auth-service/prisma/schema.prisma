generator client {
  provider = "prisma-client-js"
  url      = env("DATABASE_URL")
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String    @id @default(cuid())
  username  String    @unique
  email     String    @unique
  password  String    @unique
  createdAt DateTime  @default(now())
}
